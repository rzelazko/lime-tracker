{
  "name": "lime-tracker",
  "version": "1.0.24",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=en --host 0.0.0.0 --disable-host-check",
    "start-pl": "ng serve --configuration=pl --host 0.0.0.0 --disable-host-check",
    "build": "ng build --delete-output-path",
    "postbuild": "node scripts/postbuild.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test-debug": "ng test --browsers=ChromeDebug",
    "firebase-emulator": "firebase emulators:start --only firestore,auth --import test-data",
    "i18n-extract": "ng extract-i18n --output-path src/locale"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.1",
    "@angular/cdk": "^17.3.1",
    "@angular/common": "^17.3.1",
    "@angular/compiler": "^17.3.1",
    "@angular/core": "^17.3.1",
    "@angular/fire": "^17.0.1",
    "@angular/forms": "^17.3.1",
    "@angular/material": "^17.3.1",
    "@angular/material-moment-adapter": "^17.3.1",
    "@angular/platform-browser": "^17.3.1",
    "@angular/platform-browser-dynamic": "^17.3.1",
    "@angular/router": "^17.3.1",
    "@angular/service-worker": "^17.3.1",
    "apexcharts": "^3.42.0",
    "firebase": "^10.8.0",
    "firebase-admin": "^12.0.0",
    "moment": "^2.29.4",
    "moment-range": "^4.0.2",
    "ng-apexcharts": "^1.8.0",
    "rxfire": "^6.0.5",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.1",
    "@angular/cli": "^17.3.1",
    "@angular/compiler-cli": "^17.3.1",
    "@angular/localize": "^17.3.1",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^20.11.16",
    "firebase-tools": "^13.4.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~5.3.3",
    "xml2js": "^0.6.2"
  }
}