{
  "name": "lime-tracker",
  "version": "1.0.45",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=en --host 0.0.0.0 --disable-host-check",
    "start-pl": "ng serve --configuration=pl --host 0.0.0.0 --disable-host-check",
    "build": "ng build --delete-output-path",
    "postbuild": "node scripts/postbuild.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "firebase-emulator": "firebase emulators:start --only firestore,auth --import test-data",
    "i18n-extract": "ng extract-i18n --output-path src/locale"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.4",
    "@angular/cdk": "^19.0.3",
    "@angular/common": "^19.0.4",
    "@angular/compiler": "^19.0.4",
    "@angular/core": "^19.0.4",
    "@angular/fire": "^18.0.1",
    "@angular/forms": "^19.0.4",
    "@angular/material": "^19.0.3",
    "@angular/material-moment-adapter": "^19.0.3",
    "@angular/platform-browser": "^19.0.4",
    "@angular/platform-browser-dynamic": "^19.0.4",
    "@angular/router": "^19.0.4",
    "@angular/service-worker": "^19.0.4",
    "apexcharts": "^4.2.0",
    "firebase": "^11.1.0",
    "firebase-admin": "^13.0.1",
    "moment": "^2.30.1",
    "moment-range": "^4.0.2",
    "ng-apexcharts": "^1.15.0",
    "rxfire": "^6.1.0",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.5",
    "@angular/cli": "^19.0.5",
    "@angular/compiler-cli": "^19.0.4",
    "@angular/localize": "^19.0.4",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^22.5.5",
    "firebase-tools": "^13.6.0",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "5.5",
    "xml2js": "^0.6.2"
  }
}
