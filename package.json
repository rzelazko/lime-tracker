{
  "name": "lime-tracker",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=en --host 0.0.0.0 --disable-host-check",
    "start-pl": "ng serve --configuration=pl --host 0.0.0.0 --disable-host-check",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test-debug": "ng test --browsers=ChromeDebug",
    "firebase-emulator": "firebase emulators:start --only firestore,auth --import test-data",
    "i18n-extract": "ng extract-i18n --output-path src/locale"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.5",
    "@angular/cdk": "^16.2.4",
    "@angular/common": "^16.2.5",
    "@angular/compiler": "^16.2.5",
    "@angular/core": "^16.2.5",
    "@angular/fire": "^7.4.1",
    "@angular/forms": "^16.2.5",
    "@angular/material": "^16.2.4",
    "@angular/material-moment-adapter": "^16.2.4",
    "@angular/platform-browser": "^16.2.5",
    "@angular/platform-browser-dynamic": "^16.2.5",
    "@angular/router": "^16.2.5",
    "@angular/service-worker": "^16.2.5",
    "apexcharts": "^3.33.1",
    "firebase": "^9.4.0",
    "firebase-admin": "^11.4.1",
    "moment": "^2.29.4",
    "moment-range": "^4.0.2",
    "ng-apexcharts": "^1.7.0",
    "rxfire": "^6.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.2",
    "@angular/cli": "^16.2.2",
    "@angular/compiler-cli": "^16.2.5",
    "@angular/localize": "^16.2.5",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~5.1.6"
  }
}
